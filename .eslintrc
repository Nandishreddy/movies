{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules":
  {
    "no-undef": 0,
    "quotes": [ 2, "single" ],
    "no-console": 0,
    "no-alert": 0,
    "max-len": [1, 1000, 2,
      {
        "ignoreComments": true
      }],
    "react/prop-types": [0]
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}